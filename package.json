{
  "name": "qwuack",
  "version": "0.1.0",
  "description": "Redis-backed ledger with running totals optimization",
  "type": "module",
  "main": "./dist/ledger.js",
  "types": "./dist/ledger.d.ts",
  "exports": {
    ".": {
      "types": "./dist/ledger.d.ts",
      "import": "./dist/ledger.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "bun run build.ts",
    "test": "bun test",
    "test:coverage": "bun test --coverage",
    "prepublishOnly": "bun run build"
  },
  "keywords": [
    "redis",
    "ledger",
    "accounting",
    "financial",
    "balance"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/arifsetyawan/qwuack.git"
  },
  "bugs": {
    "url": "https://github.com/arifsetyawan/qwuack/issues"
  },
  "homepage": "https://github.com/arifsetyawan/qwuack#readme",
  "devDependencies": {
    "@types/bun": "latest"
  },
  "peerDependencies": {
    "typescript": "^5",
    "redis": "^5.0.0",
    "ioredis": "^5.0.0"
  },
  "peerDependenciesMeta": {
    "redis": {
      "optional": true
    },
    "ioredis": {
      "optional": true
    }
  },
  "dependencies": {
    "bignumber.js": "^9.3.1"
  }
}
